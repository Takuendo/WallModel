FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha 0
);

regions
(
    boxToCell 
    {
      box
	(0 0.15 0) (8 0.2 4);
        fieldValues
        (
             volScalarFieldValue alpha 1
        );
    }

);


